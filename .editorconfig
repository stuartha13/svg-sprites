# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
indent_size = 2
charset = utf-8
end_of_line = lf
indent_style = space
insert_final_newline = true

[*.scss]
insert_final_newline = true

# Tab indentation (no size specified)
[Makefile]
indent_style = space
